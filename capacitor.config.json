{
  "appId": "com.trundle.app",
  "appName": "Trundle",
  "webDir": "out",
  "bundledWebRuntime": false,
  "server": {
    "androidScheme": "https"
  },
  "plugins": {
    "SplashScreen": {
      "launchShowDuration": 2000,
      "backgroundColor": "#F6F0E8",
      "showSpinner": false
    },
    "Geolocation": {
      "permissions": ["location"]
    }
  },
  "android": {
    "buildOptions": {
      "keystorePath": "",
      "keystoreAlias": ""
    }
  }
}
